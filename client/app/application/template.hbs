<div class="container">
  <div class="row">
    <div class="col s3">
      <h4>Contacts</h4>
      <ul>
      {{#each contacts as |contact|}}
        <li>{{link-to contact.displayName 'contact' contact}}</li>
      {{else}}
        No contacts
      {{/each}}
      </ul>
    </div>

    <div class="col s9">
      {{outlet}}
    </div>
  </div>
</div>
